    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

    <repositories>
        <repository>
            <id>jitpack.io</id>
            <url>https://jitpack.io</url>
        </repository>
    </repositories>

    <profiles>
        <profile>
            <id>dev</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <!--project-->
                <!--properties如果有中文, 先转换成unicode, 如: 你好 => \u4f60\u597d-->
                <pom.project.profile>dev</pom.project.profile>
                <pom.server.port>20000</pom.server.port>
                <!--database-->
                <pom.db.host>localhost</pom.db.host>
                <pom.db.port>3306</pom.db.port>
                <pom.db.database>dev_blog</pom.db.database>
                <pom.db.username>test</pom.db.username>
                <pom.db.password>123456</pom.db.password>
                <!--web index-->
                <pom.project.web.index>http://blog.loc:5000/blog-web/</pom.project.web.index>
                <!--mail-->
                <pom.postfix.host>smtp.163.com</pom.postfix.host>
                <pom.postfix.port>465</pom.postfix.port>
                <pom.postfix.protocol>smtps</pom.postfix.protocol>
