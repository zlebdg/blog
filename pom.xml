                <pom.security.oauth2.client.client-id>client</pom.security.oauth2.client.client-id>
                <pom.security.oauth2.client.client-secret>secret</pom.security.oauth2.client.client-secret>
                <pom.security.oauth2.server.domain>http://auth.loc:20010</pom.security.oauth2.server.domain>
                <pom.security.oauth2.client.pre-established-redirect-uri>http://blog.loc:5000/blog/login
                </pom.security.oauth2.client.pre-established-redirect-uri>
                <pom.project.redis>spring.redis.host=localhost</pom.project.redis>
            </properties>
